<div class="profile">

  <div class="padding-m border-radius background-white profile-page box-shadow-xl margin-top-xxl margin-bottom-xxl">
    <h1 class="color-dark margin-top-m text-center display-title display-title-xl padding-bottom-m">My Information</h1>


      <% if(data) { %>
    <ul class="profile-info">
      <li><h1>Name: </h1> <p><%= data.name %> </p> </li>
      <li><h1>Address: </h1> <p><%= data.address %> </p> </li>
      <li><h1>Second Address: </h1> <p><%= data.address2 %> </p> </li>
      <li><h1>City: </h1> <p><%= data.city %> </p> </li>
      <li><h1>Zipcode: </h1> <p><%= data.zipcode %> </p> </li>
      <% if(data.state == 1) { %>
      <li><h1>Zipcode: </h1> <p>Texas </p> </li>
       <% } else{ %>
      <li><h1>Zipcode: </h1> <p>Out-of-State</p> </li>
       <% } %>

    </ul>
       <% } else{ %>
          <h1> Please fill out information in the profile page </h1> 
       <% } %>

  </div>



</div>